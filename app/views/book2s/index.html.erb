<p id="notice"><%= notice %></p>

<h1>Book2s</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Author</th>
      <th>Price</th>
      <th>Published date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @book2s.each do |book2| %>
      <tr>
        <td><%= book2.title %></td>
        <td><%= book2.author %></td>
        <td><%= book2.price %></td>
        <td><%= book2.published_date %></td>
        <td><%= link_to 'Show', book2 %></td>
        <td><%= link_to 'Edit', edit_book2_path(book2) %></td>
        <td><%= link_to 'Destroy', book2, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Book2', new_book2_path %>
