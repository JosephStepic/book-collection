<p id="notice"><%= notice %></p>

<h1>Book3s</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Author</th>
      <th>Price</th>
      <th>Published date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @book3s.each do |book3| %>
      <tr>
        <td><%= book3.title %></td>
        <td><%= book3.author %></td>
        <td><%= book3.price %></td>
        <td><%= book3.published_date %></td>
        <td><%= link_to 'Show', book3 %></td>
        <td><%= link_to 'Edit', edit_book3_path(book3) %></td>
        <td><%= link_to 'Destroy', book3, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Book3', new_book3_path %>
